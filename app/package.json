{
  "name": "app",
  "version": "0.1.0",
  "private": true,
  "dependencies": {
    "@hookform/resolvers": "^1.3.5",
    "@testing-library/jest-dom": "^5.11.9",
    "@testing-library/react": "^11.2.5",
    "@testing-library/user-event": "^12.8.3",
    "axios": "^0.21.1",
    "json-server": "^0.16.3",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-google-charts": "^3.0.15",
    "react-hook-form": "^6.15.4",
    "react-router": "^5.2.0",
    "react-router-dom": "^5.2.0",
    "react-scripts": "4.0.3",
    "react-test-renderer": "^16.13.1",
    "styled-components": "^5.2.3",
    "web-vitals": "^1.1.0",
    "yup": "^0.32.9"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "stop": "react-scripts stop",
    "test": "react-scripts test",
    "test:cov": "react-scripts test --coverage --coverageDirectory=src/coverage --watchAll=false & json-server -p 6000 src/mocks/api.json env.test",
    "eject": "react-scripts eject",
    "lint": "eslint --ignore-path .gitignore --ext .js src/",
    "lint:fix": "eslint --single-quote --trailing-comma all --ext .js src/ --fix",
    "format": "prettier --single-quote --trailing-comma all --check ./src",
    "format:fix": "prettier --single-quote --trailing-comma all --write ./src"
  },
  "jest": {
    "coverageReporters": [
      "text",
      "html"
    ],
    "coveragePathIgnorePatterns": [
      "coverage",
      "src/index.js"
    ]
  },
  "eslintConfig": {
    "extends": [
      "react-app",
      "react-app/jest"
    ]
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "devDependencies": {
    "@testing-library/react-hooks": "^5.1.2",
    "eslint": "^7.25.0",
    "eslint-config-airbnb": "^18.2.1",
    "eslint-config-prettier": "^8.3.0",
    "eslint-plugin-import": "^2.22.1",
    "eslint-plugin-jsx-a11y": "^6.4.1",
    "eslint-plugin-prettier": "^3.4.0",
    "eslint-plugin-react": "^7.23.2",
    "eslint-plugin-react-hooks": "^4.2.0",
    "msw": "^0.28.2",
    "prettier": "^2.2.1"
  }
}
